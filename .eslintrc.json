{
  "extends": ["next/core-web-vitals", "prettier", "plugin:prettier/recommended"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["**/*.md", "**/*.json", "**/.*"],
  "rules": {
    "prettier/prettier": "error",
    "camelcase": "error",
    "no-duplicate-imports": "error"
  }
}
